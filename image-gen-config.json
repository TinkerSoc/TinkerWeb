{
  "Paths": [
    {
      "Path": "./images/",
      "Destination": "./static/gen/images",
      "Ignore": "./images/committee",
      "Recursive": true,
      "Resize": [
        {
          "Algorithm": "Box",
          "Suffix": "-large",
          "Width": 960,
          "Height": 960,
          "KeepAspectRatio": true
        },
        {
          "Algorithm": "Box",
          "Suffix": "-medium",
          "Width": 480,
          "Height": 480,
          "KeepAspectRatio": true
        },
        {
          "Algorithm": "Box",
          "Suffix": "-small",
          "Width": 320,
          "Height": 320,
          "KeepAspectRatio": true
        }
      ]
    },
    {
      "Path": "./images/committee",
      "Destination": "./static/gen/images/committee",
      "Ignore": "",
      "Recursive": true,
      "Resize": [
        {
          "Algorithm": "Box",
          "Suffix": "-large",
          "Width": 960,
          "Height": 960,
          "KeepAspectRatio": true
        },
        {
          "Algorithm": "Box",
          "Suffix": "-medium",
          "Width": 480,
          "Height": 480,
          "KeepAspectRatio": true
        },
        {
          "Algorithm": "Box",
          "Suffix": "-small",
          "Width": 320,
          "Height": 320,
          "KeepAspectRatio": true
        }
      ]
    }
  ]
}
